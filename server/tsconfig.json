{
	"compilerOptions": {
		"moduleResolution": "nodenext",
		"module": "nodenext",
		"esModuleInterop": true,
		"target": "es2018",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist",
		"rootDir": "./server",
		"typeRoots": ["./node_modules/@types", "./types"]
	},
	"typeRoots": ["./node_modules/@types", "./server/types"],
	"include": ["./server/src/**/*", "./types/**/*.d.ts"]
}
