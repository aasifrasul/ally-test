{
	"name": "ally-test",
	"main": "server/src/index.js",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"lint:css": "stylelint 'src/**/*.css'",
		"lint:js": "eslint . --ext **/*.js,**/*.jsx,**/*.ts,**/*.tsx,**/*.hbs",
		"lint:fix": "eslint --fix",
		"prettier": "npx prettier --write './**/*.{js,json,jsx,ts,tsx,css}'",
		"build:client": "yarn build:make",
		"build-prod": "yarn build:make",
		"ci-node": "yarn build:make && sh ./deployer/ci-node.sh",
		"ci-web": "sh ./deployer/ci-web.sh",
		"validate": "npm ls",
		"build:make": "node --trace-deprecation make/make.js",
		"watch": "npx babel src --out-dir public --watch",
		"test:server": "ts-node server/server.ts",
		"dev": "nodemon",
		"dev:server": "ts-node server/src/index.ts",
		"build:server": "tsc -p server/tsconfig.json",
		"watch:server": "tsx watch server/src/index.ts",
		"dev:hmr": "cross-env NODE_ENV=development webpack serve --config webpack-configs/webpack.config.js --hot --open",
		"prod:build:client": "cross-env NODE_ENV=production webpack --config webpack-configs/webpack.config.js",
		"test": "jest",
		"cy:open": "cypress open",
		"coverage": "jest --coverage",
		"test:e2e": "npx playwright test",
		"test:headed": "npx playwright test --headed",
		"test:report": "npx playwright show-report"
	},
	"browser": {
		"fs": false,
		"os": false,
		"path": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"**/*.{jsx,ts,tsx,hbs}": [
			"prettier --write"
		],
		"src/**/*.css": [
			"stylelint --fix",
			"prettier --write"
		],
		"**/*.{json,yml,yaml,md,mdx}": [
			"prettier --write"
		]
	},
