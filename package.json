{
	"name": "ally-test",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"lint:css": "stylelint 'src/**/*.css'",
		"lint:js": "eslint . --ext **/*.js,**/*.jsx,**/*.ts,**/*.tsx,**/*.hbs",
		"prettier": "prettier --write './**/*.{js,json,jsx,ts,tsx,css}'",
		"build": "PROMO_ENV=next1 yarn build:make",
		"build-prod": "PROMO_ENV=prod yarn build:make",
		"ci-node": "yarn build:make && sh ./deployer/ci-node.sh",
		"ci-web": "sh ./deployer/ci-web.sh",
		"validate": "npm ls",
		"build:make": "node --trace-deprecation make/make.js",
		"start": "nodemon server/server.js",
		"start-dev": "NODE_ENV=development BABEL_ENV=development nodemon server.js nodemon server.js",
		"build-dev": "NODE_ENV=development BABEL_ENV=development webpack",
		"test": "nodemon scripts/test.js"
	},
	"proxy": "http://my.flipkart.com:3000",
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.+(js|jsx|ts|tsx)": [
			"yarn lint:js",
			"yarn prettier"
		],
		"**/*.+(json|yml|yaml|css|less|scss|md|mdx)": [
			"yarn prettier"
		]
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.3",
		"async": "3.2.4",
		"axios": "1.3.6",
		"browserslist": "^4.21.5",
		"caniuse-lite": "^1.0.30001481",
		"fomantic-ui-css": "2.9.2",
		"idx": "2.5.6",
		"keypress.js": "2.1.5",
		"lint-staged": "11.1.2",
		"mongoose": "5.12.13",
		"prop-types": "15.7.2",
		"react": "^18.2.0",
		"react-data-grid": "^7.0.0-beta.28",
		"react-dom": "^18.2.0",
		"react-icons": "^4.8.0",
		"react-query": "3.39.3",
		"react-redux": "8.0.5",
		"react-router-dom": "6.10.0",
		"redux-devtools-extension": "^2.13.9",
		"redux-saga": "1.2.3",
		"regenerator-runtime": "0.13.11",
		"semantic-ui-react": "2.1.4",
		"serve-static": "^1.15.0",
		"socket.io": "4.6.1",
		"socket.io-client": "4.6.1",
		"styled-components": "5.3.10",
		"winston": "^3.8.2"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@babel/plugin-proposal-class-properties": "7.18.6",
		"@babel/plugin-proposal-json-strings": "7.18.6",
		"@babel/plugin-proposal-private-methods": "7.18.6",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-syntax-import-meta": "7.10.4",
		"@babel/plugin-transform-runtime": "7.21.4",
		"@babel/preset-env": "7.21.4",
		"@babel/preset-react": "7.18.6",
		"@babel/preset-typescript": "7.21.4",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"awesome-typescript-loader": "5.2.1",
		"babel-core": "6.26.3",
		"babel-eslint": "10.1.0",
		"babel-loader": "8.0.5",
		"babel-plugin-macros": "3.1.0",
		"babel-plugin-named-asset-import": "0.3.8",
		"clean-webpack-plugin": "4.0.0",
		"compression-webpack-plugin": "10.0.0",
		"cookie-parser": "1.4.6",
		"cors": "2.8.5",
		"css-loader": "^6.7.3",
		"eslint": "^8.39.0",
		"eslint-loader": "2.1.1",
