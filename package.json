{
	"name": "ally-test",
	"main": "server/src/index.js",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"yarn build:client\" \"yarn dev:server\"",
		"lint:css": "stylelint 'src/**/*.css'",
		"lint:js": "eslint . --ext **/*.js,**/*.jsx,**/*.ts,**/*.tsx,**/*.hbs",
		"prettier": "prettier --write './**/*.{js,json,jsx,ts,tsx,css}'",
		"build:client": "yarn build:make",
		"build-prod": "yarn build:make",
		"ci-node": "yarn build:make && sh ./deployer/ci-node.sh",
		"ci-web": "sh ./deployer/ci-web.sh",
		"validate": "npm ls",
		"build:make": "node --trace-deprecation make/make.js",
		"watch": "npx babel src --out-dir public --watch",
		"start": "nodemon server/dist/index.js",
		"dev:server": "ts-node server/src/index.ts",
		"build:server": "tsc -p server/tsconfig.json",
		"watch:server": "tsc -w -p server/tsconfig.json",
		"test": "jest",
		"cy:open": "cypress open",
		"coverage": "jest --coverage",
		"pre-commit": "yarn lint-staged"
	},
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.+(js|jsx|ts|tsx)": [
			"yarn prettier"
		],
		"**/*.+(json|yml|yaml|css|less|scss|md|mdx)": [
			"yarn prettier"
		]
	},
